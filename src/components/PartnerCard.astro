---
interface Props {
  text: string;
  imageURL: string;
}
const { text = "", imageURL = "" } = Astro.props;
const imageStyle = {
  backgroundImage: `url(/logos/${imageURL})`,
};
const imageSrc = `/logos/${imageURL}`;
---

<img src={imageSrc} alt="{{text}}" class="w-48 h-36 object-contain p-2 m-8 center" />